<AnchorPane prefHeight="300.0" prefWidth="400.0" stylesheets="@manager.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.xidian.view.LoginController">
